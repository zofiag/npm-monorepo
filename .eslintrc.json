{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended","plugin:@typescript-eslint/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
   "env": {
    "node": true,
    "es2020": true
  },
   "overrides": [
    {
      "parser": "@typescript-eslint/parser",
      "files": [
        "*.ts",
        "*.tsx"
      ]
    }
  ]
}